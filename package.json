{
  "name": "bambin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest run --config tsconfig/vitest.config.ts --coverage frontend/src",
    "test:e2e": "playwright test --config=tsconfig/playwright.config.ts",
    "build-only": "vite build",
    "type-check": "vue-tsc --build tsconfig/tsconfig.app.json",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "db:generate": "drizzle-kit generate --config tsconfig/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config tsconfig/drizzle.config.ts",
    "db:push": "drizzle-kit push --config tsconfig/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config tsconfig/drizzle.config.ts",
    "db:pull": "drizzle-kit introspect --config tsconfig/drizzle.config.ts",
    "start": "tsx start.ts",
    "test:db": "cross-env DATABASE_URL=$DATABASE_URL_TEST vitest run --config tsconfig/vitest.config.ts --run api/db/__tests__"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@supabase/supabase-js": "^2.50.0",
    "bcrypt": "^6.0.0",
    "bulma": "^1.0.4",
    "drizzle-orm": "^0.44.2",
    "gel": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "pg": "^8.16.0",
    "postgres": "^3.4.7",
    "vue": "^3.5.13",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.5.1",
    "vue3-lazyload": "^0.3.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.21",
    "@types/pg": "^8.15.4",
    "@vercel/node": "^5.2.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.2.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.22.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.2.2",
    "vue-tsc": "^2.2.8"
  }
}
